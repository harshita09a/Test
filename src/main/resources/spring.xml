<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.3.xsd">

	<bean id="emp" class="com.cg.lab1.b.Employee">
		<property name="employeeName" value="Harshita Agrawal"/>
		<property name="employeeid" value="23455"/>
		<property name="businessUnit" ref="sbu"/>
		
		<property name="salary" value="25000"/>
	</bean>
	<bean id="sbu" class="com.cg.lab1.b.SBU">
		<property name="SbuId" value="345"/>
		<property name="SbuName" value="Engineering Services"/>
		<property name="SbuHead" value="Kiran Rao"/>
	</bean>
</beans>